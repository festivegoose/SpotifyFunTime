{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SpotifySettings": {
    "AccountBaseUri": "https://accounts.spotify.com",
    "ApiBaseUri": "https://api.spotify.com/v1",
    "RedirectUri": "http://localhost:5000/sign-in",
    "Scope": "user-library-read user-top-read user-read-recently-played user-follow-read"
  }
}
